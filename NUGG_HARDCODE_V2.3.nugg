   <parameter CLSNAME="ZCL_HARDCODE_MAINTAIN" CMPNAME="VERSION_GET" SCONAME="ES_HARDCODE_VERSION" VERSION="1" LANGU="F" DESCRIPT="HC - Entête table de Customizing" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="TY_HARDCODE_VERSION"/>
